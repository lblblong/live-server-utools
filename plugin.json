{
  "pluginName": "Live Server",
  "description": "启动静态资源实时服务，静态页面开发神器。",
  "author": "lblblong",
  "homepage": "https://github.com/lblblong/live-server-utools",
  "main": "./build/index.html",
  "preload": "./preload/dist/index.js",
  "version": "0.0.1",
  "logo": "assets/logo.png",
  "platform": ["win32"],
  "features": [
    {
      "code": "startLiveServer",
      "explain": "静态资源实时服务管理面板",
      "cmds": [
        "Live Server",
        {
          "type": "window",
          "label": "启动实时文件服务",
          "match": {
            "app": ["Finder.app", "explorer.exe"]
          }
        }
      ]
    }
  ]
}
